@model IEnumerable<firstapproch.Models.Student>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<table>
    <tr>
        <th>id</th>
        <th>Name</th>
        <th>Class</th>
        <th>Marks</th>


    </tr>
    @{ foreach (var item in Model)
        {
            <tr>@item.id</tr>
            <tr>@item.name</tr>
            <tr>@item.classes</tr>
            <tr>@item.marks</tr>
        }
    }
</table>

